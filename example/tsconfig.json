{
  "extends": "../tsconfig",
  "compilerOptions": {},
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "paths": {
    "@/*": [
      "./*"
    ],
    "components/*": [
      "./components/*",
      "./components/**/*.tsx",
      "./components/**/**/*.tsx",
      "./components/**/**/_*.tsx",
      "./app/(tabs)/_layout.tsx",
      "./components/**/+*.tsx",
    ],
  }
}